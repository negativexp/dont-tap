<Window x:Class="FetchJsonFromWebsite.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FetchJsonFromWebsite"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="500">
    <Grid>
        <Border BorderThickness="2" BorderBrush="Black" Width="375" Height="375">
            <TextBlock Name="TextBoxData"/>
        </Border>
        <Button VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Height="20" Content="Fetch" Margin="20" Click="Button_Click"/>
    </Grid>
</Window>
